{
  "expo": {
    "name": "GatorRide",
    "slug": "GatorRide",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/gatorride.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash":
    {
      "image": "./assets/images/gatorride.png",
      "imageWidth": 200,
      "resizeMode": "contain",
      "backgroundColor": "#0b1e7d"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/gatorride.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/gatorride.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/gatorride.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0b1e7d"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
